<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <PublishProvider>AzureWebSite</PublishProvider>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://slkdevelop.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>slkdevelop.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>SlkDevelop</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$SlkDevelop</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DefaultConnection" Order="1" Enabled="False">
          <Destination Path="Data Source=tcp:slkdevelop.database.windows.net,1433;Initial Catalog=SlkDevelop;User ID=slk-developer@slkdevelop.database.windows.net;Password=DasPassword5" Name="Data Source=tcp:slkdevelop.database.windows.net,1433;Initial Catalog=SlkDevelop;User Id=slk-developer@slkdevelop.database.windows.net;Password=DasPassword5" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="SLK.DataLayer.ApplicationDbContext, SLK.DataLayer" MigrationConfiguration="SLK.DataLayer.Migrations.Configuration, SLK.DataLayer" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DefaultConnection-Web.config Connection String">
      <ParameterValue>Data Source=tcp:slkdevelop.database.windows.net,1433;Initial Catalog=SlkDevelop;User Id=slk-developer@slkdevelop.database.windows.net;Password=DasPassword5</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>